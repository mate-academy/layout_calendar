{
  "elementspanider": "__",
  "modifierspanider": "--",
  "ignore": [
    "node_modules",
    "dist"
  ],
  "rules": {
    "one-cell": true,
    "one-element": true,
    "element-inside-parent-cell": true,
    "no-double-element": true,
    "no-neighbour-parent-cell": true,
    "modifiable-class": true
  }
}
